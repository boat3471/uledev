#!/usr/bin/env node
var args = process.argv.slice(2);
var usage = [ ''
	, '  Options:'
	, '    -h, --help              Display help information'
	, ''
].join('\n');
var arg;
while (args.length) {
	arg = args.shift();
	switch (arg) {
		case '-h':
		case '-help':
			console.error(usage);
			break;
		case '':
			break;
	}
}